alias ll='ls -laG'
alias runworker='celery -A eshares.celery_config worker -Q whenever,user_waiting,longtasks,default --concurrency=8 -l INFO'
alias runserver='workon eshares && python manage.py runserver'
alias goshares='cd $HOME/src/eshares-certificates && workon eshares'
alias relias='source $HOME/.aliases'
alias manage='python manage.py'
alias clear-branches='for branch in `git branch | egrep -v \s?(master|develop).*$ | xargs`; do git branch -D $branch; done'
